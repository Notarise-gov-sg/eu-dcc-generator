{
  "compilerOptions": {
    "resolveJsonModule": true,
    "declaration": true,
    "outDir": "./dist",
    "lib": ["ES2021"],
    "module": "commonjs",
    "target": "es2021",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  },
  "include": ["src"],
  "exclude": ["src/**/*.test.ts","node_modules"],
}
