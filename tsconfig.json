{
  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJS",
    "resolveJsonModule": true,
    "declaration": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "src/**/*.test.ts"]
}
